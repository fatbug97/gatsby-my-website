{
  "name": "my-site",
  "version": "1.0.0",
  "private": true,
  "description": "my-site",
  "author": "Thinh Le Minh",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "predeploy": "gatsby build",
    "deploy": "yarn predeploy && echo 'thinh.xyz' > ./public/CNAME && gh-pages -d public",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "sortier": "sortier src",
    "prepare": "husky install",
    "eslint": "eslint --ext .tsx --ext .ts src/ --quiet --fix"
  },
  "dependencies": {
    "@loadable/component": "5.15.2",
    "babel-plugin-styled-components": "2.0.6",
    "gatsby": "4.10.3",
    "gatsby-plugin-image": "2.10.1",
    "gatsby-plugin-manifest": "4.10.2",
    "gatsby-plugin-react-helmet": "5.10.0",
    "gatsby-plugin-sharp": "4.10.2",
    "gatsby-plugin-sitemap": "5.10.2",
    "gatsby-plugin-styled-components": "5.10.0",
    "gatsby-source-filesystem": "4.10.1",
    "gatsby-transformer-sharp": "4.10.0",
    "moment": "^2.29.1",
    "moment-lunar": "^0.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.12.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "gh-pages": "^3.2.3",
    "husky": "7.0.4",
    "lint-staged": ">=12",
    "sortier": "1.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "volta": {
    "node": "16.13.1"
  },
  "lint-staged": {
    "src/**/*.tsx": [
      "yarn sortier",
      "yarn eslint"
    ],
    "src/**/*.ts": [
      "yarn sortier",
      "yarn eslint"
    ],
    "src/**/*.json": [
      "yarn sortier",
      "yarn eslint"
    ]
  }
}
